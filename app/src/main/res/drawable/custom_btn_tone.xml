<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape>
            <solid
                android:color="#19ffd1"/>
            <corners
                android:radius="60dp"/>
        </shape>
    </item>
    <item>
        <shape>

            <solid
                android:color="#ffffff"/>
            <stroke
                android:color="#ffffff"/>
            <corners
                android:radius="60dp"/>
        </shape>
    </item>
</selector>